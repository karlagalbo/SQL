--create a clothing store database

CREATE TABLE clothing_store (id INTEGER PRIMARY KEY, item TEXT, price INTEGER, color TEXT, size INTEGER);

INSERT INTO clothing_store VALUES (1, "tshirt", 10,"white", 3);
